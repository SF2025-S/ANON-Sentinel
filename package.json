{
  "name": "aiserver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --project tsconfig.server.json --ignore-watch='.next' src/server/index.ts",
