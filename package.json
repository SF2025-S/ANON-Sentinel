    "@radix-ui/react-slot": "^1.1.1",
    "@tailwindcss/typography": "github:tailwindcss/typography",
    "ai": "^4.1.61",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "express": "^4.21.2",
